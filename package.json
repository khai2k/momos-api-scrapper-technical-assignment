{
  "name": "web-scraper-api",
  "version": "1.0.0",
  "description": "Simple Express.js API for scraping images and videos from web URLs",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node server.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "test:scraping": "node test-scraping.js",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:migrate": "docker-compose exec api npm run migration:run",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/database/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/database/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/database/data-source.ts"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "typescript": "^5.3.0",
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/cheerio": "^0.22.35",
    "@types/pg": "^8.10.9",
    "ts-node": "^10.9.0",
    "rimraf": "^5.0.5"
  },
  "keywords": [
    "express",
    "scraping",
    "api",
    "images",
    "videos",
    "typeorm",
    "postgresql",
    "docker"
  ],
  "author": "",
  "license": "MIT"
}